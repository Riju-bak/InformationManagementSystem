@model IEnumerable<InformationManagementSystem.Models.SchoolViewModels.EnrollmentDateGroup>

@{
    ViewData["Title"] = "About";
}

<h1>Student Body Statistics</h1>

<table class="table">
    <thead>
    <tr>
        <th>
            Enrollment Date
        </th>
        <th>
            Students
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @* @Html.DisplayFor(modelItem => item.EnrollmentDate) *@
                @item.EnrollmentDate
            </td>
            <td>
                @* @Html.DisplayFor(modelItem => item.StudentCount) *@
                @item.StudentCount
            </td>
        </tr>
    }
    </tbody>
</table>